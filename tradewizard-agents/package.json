{
  "name": "tradewizard-agents",
  "version": "1.0.0",
  "description": "Market Intelligence Engine - Multi-agent system for prediction market analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "prediction-markets",
    "ai-agents",
    "langgraph",
    "polymarket"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/anthropic": "^1.3.8",
    "@langchain/core": "^1.1.13",
    "@langchain/google-genai": "^2.1.8",
    "@langchain/langgraph": "^1.0.15",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/openai": "^1.2.2",
    "@polymarket/clob-client": "^5.2.1",
    "@supabase/supabase-js": "^2.90.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "opik": "^1.9.81",
    "opik-langchain": "^1.9.82",
    "ora": "^8.1.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "fast-check": "^4.5.3",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
